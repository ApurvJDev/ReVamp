#include<bits/stdc++.h>
using namespace std;

void check(vector<int>& nums) {
    
    unordered_set<int>unique;
    for(int i : nums) {
        cout << i << ':' << unique.count(i) << '\n';
        unique.insert(i);
    }
}
int main() {
    vector<int> nums = {1,1,1,3,3,4,3,2,4,2};
    check(nums);
    return 0;
}
