#include<bits/stdc++.h>
using namespace std;

int main() {
     string s = "racecar";
     map<char, int> sMap;
     
     for(char ch : s) {
         sMap[ch]++;
     }
     
      for(const auto& pair : sMap) {
         cout << pair.first << ":" << pair.second << '\n';
     }
     return 0;
 }

op:

1:0
1:1
1:1
3:0
3:1
4:0
3:1
2:0
4:1
2:1

